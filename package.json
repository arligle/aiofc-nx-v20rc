{
  "name": "@aiofc-nx/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "test:all": "nx affected:test --all --codeCoverage --skip-nx-cache",
    "prepare": "husky install",
    "lint:ws": "pnpm dlx sherif@latest",
    "lint:ws-fix": "pnpm dlx sherif@latest --fix",
    "clean": "git clean -xdf node_modules",
    "clean-all": "pnpm run clean && pnpm recursive exec rm -rf node_modules"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/config-nx-scopes": "^19.5.0",
    "@nestjs/schematics": "^10.0.1",
    "@nx/devkit": "20.0.0-rc.0",
    "@nx/eslint": "20.0.0-rc.0",
    "@nx/jest": "20.0.0-rc.0",
    "@nx/js": "20.0.0-rc.0",
    "@nx/nest": "20.0.0-rc.0",
    "@nx/node": "20.0.0-rc.0",
    "@nx/plugin": "20.0.0-rc.0",
    "@nx/web": "20.0.0-rc.0",
    "@nx/webpack": "20.0.0-rc.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "commitizen": "^4.3.1",
    "cz-customizable": "^7.2.1",
    "eslint": "^9.13.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "nx": "20.0.0-rc.0",
    "tslib": "^2.3.0",
    "typescript": "~5.5.2",
    "webpack-cli": "^5.1.4"
  }
}
